FoamFile
{
    format      ascii;
    class       dictionary;
    location    "constant";
    object      transportProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

phases (water air);

water
{
    transportModel  Newtonian;
    nu              1.1234e-06; // At 15.5 deg C
    rho             998.8;
}

air
{
    transportModel  Newtonian;
    nu              1.466e-05;  // At 15.5 deg C
    rho             1;
}

sigma           0;

