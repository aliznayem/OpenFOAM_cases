{
    body
    {
        type            zeroGradient;
    }
    closed
    {
        type            zeroGradient;
    }
    opened
    {
        type            zeroGradient;
    }
    hole0
    {
        type            fixedValue;
        value           uniform 0;
    }
    hole1
    {
        type            zeroGradient;
    }
    hole2
    {
        type            zeroGradient;
    }
    hole3
    {
        type            zeroGradient;
    }
    hole4
    {
        type            zeroGradient;
    }
    hole5
    {
        type            zeroGradient;
    }
    hole6
    {
        type            zeroGradient;
    }
    hole7
    {
        type            zeroGradient;
    }
}

